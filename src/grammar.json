{
  "name": "Chialisp",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "mod_definition"
        }
      ]
    },
    "mod_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\(mod\\s*"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "parameter"
            },
            {
              "type": "SYMBOL",
              "name": "multiple_parameters"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "parameter": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    },
    "multiple_parameters": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "variable"
              },
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "multiple_parameters"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": ";.*\\r\\n"
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "primitive"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "function_def"
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        },
        {
          "type": "SYMBOL",
          "name": "constant_def"
        },
        {
          "type": "SYMBOL",
          "name": "constant_ref"
        },
        {
          "type": "SYMBOL",
          "name": "list_op"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "if"
        },
        {
          "type": "SYMBOL",
          "name": "raise"
        },
        {
          "type": "SYMBOL",
          "name": "ops"
        },
        {
          "type": "SYMBOL",
          "name": "quote"
        },
        {
          "type": "SYMBOL",
          "name": "builtin"
        },
        {
          "type": "SYMBOL",
          "name": "opcode"
        }
      ]
    },
    "primitive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "hexadecimal"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "hexadecimal": {
      "type": "PATTERN",
      "value": "0x[0-9a-fA-F]+"
    },
    "integer": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "string": {
      "type": "PATTERN",
      "value": "([\\\"'`])(?:[\\s\\S])*?.*([\\\"'`])"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    },
    "function_def": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((defun|defun-inline|defmacro)\\s+"
        },
        {
          "type": "SYMBOL",
          "name": "function_identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "parameter"
            },
            {
              "type": "SYMBOL",
              "name": "multiple_parameters"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "function_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_-]+"
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "function_identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "constant_def": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\(defconstant\\s+"
        },
        {
          "type": "SYMBOL",
          "name": "constant_ref"
        },
        {
          "type": "SYMBOL",
          "name": "primitive"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "constant_ref": {
      "type": "PATTERN",
      "value": "[A-Z0-9_-]+"
    },
    "list_op": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((c|f|r|l|list)\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "if": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((i|if)\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "raise": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\(x\\s*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "ops": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((=|>|>s|\\+|-|\\*|divmod)\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "quote": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((qq|q|unquote)\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "builtin": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((logand|logior|logxor|lognot|ash|lsh|substr|strlen|concat|sha256|sha256tree|point_add|pubkey_for_exp)\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "statement"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "opcode": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\((50|51|52|53|54|55|56|57|60)\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
